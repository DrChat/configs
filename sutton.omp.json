{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "type": "path",
          "background": "#E36464",
          "foreground": "#fff",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "",
          "properties": {
            "postfix": " ",
            "prefix": " ",
            "style": "full"
          }
        },
        {
          "type": "git",
          "background": "#95ffa4",
          "foreground": "#100e23",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "",
          "properties": {
            "branch_identical_icon": "\u2261",
            "cherry_pick_icon": "\u2713 ",
            "commit_icon": "\u25b7 ",
            "display_status_detail": true,
            "display_status": true,
            "display_stash_count": false,
            "display_branch_status": true,
            "local_staged_icon": "\u21d1",
            "local_working_icon": "",
            "merge_icon": "\u25f4 ",
            "no_commits_icon": "[no commits]",
            "rebase_icon": "\u2c62 ",
            "stash_count_icon": "",
            "status_colors_enabled": true,
            "status_separator_icon": " \u2502",
            "tag_icon": "\u25b6 "
          }
        },
        {
          "type": "time",
          "background": "lightMagenta",
          "foreground": "#fff",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "",
          "properties": {
            "postfix": " ",
            "prefix": " ",
            "time_format": "15:04"
          }
        },
        {
          "type": "exit",
          "background": "#2e9599",
          "foreground": "#ffffff",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "",
          "properties": {
              "always_enabled": true,
              "color_background": true,
              "display_exit_code": false,
              "error_color": "#f1184c",
              "prefix": " "
          }
        },
        {
          "type": "rust",
          "background": "#bdbdbd",
          "foreground": "#193549",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "",
          "properties": {
            "prefix": "  "
          }
        },
        {
          "type": "executiontime",
          "background": "#ff5e00",
          "foreground": "#000000",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "",
          "properties": {
            "threshold": 1000,
            "style": "austin",
            "prefix": "\u231b"
          }
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true
}

{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#E36464",
          "foreground": "#fff",
          "leading_diamond": "\ue0b6",
          "properties": {
            "style": "full"
          },
          "style": "diamond",
          "template": " {{ .Path }} ",
          "trailing_diamond": "\ue0c6",
          "type": "path"
        },
        {
          "background": "#95ffa4",
          "foreground": "#100e23",
          "leading_diamond": "\ue0c7",
          "properties": {
            "branch_identical_icon": "\u2261",
            "cherry_pick_icon": "\u2713 ",
            "commit_icon": "\u25b7 ",
            "fetch_stash_count": false,
            "fetch_status": true,
            "merge_icon": "\u25f4 ",
            "no_commits_icon": "[no commits]",
            "rebase_icon": "\u2c62 ",
            "tag_icon": "\u25b6 "
          },
          "style": "diamond",
          "template": " {{ .HEAD }} {{ .BranchStatus }}{{ if .Working.Changed }}{{ .Working.String }}{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} \u2502{{ end }}{{ if .Staging.Changed }}\u21d1{{ .Staging.String }}{{ end }}{{ if gt .StashCount 0}}{{ .StashCount }}{{ end }}{{ if gt .WorktreeCount 0}} \uf1bb {{ .WorktreeCount }}{{ end }} ",
          "trailing_diamond": "\ue0c6",
          "type": "git"
        },
        {
          "background": "lightMagenta",
          "foreground": "#fff",
          "leading_diamond": "\ue0c7",
          "properties": {
            "time_format": "15:04"
          },
          "style": "diamond",
          "template": " {{ .CurrentDate | date .Format }} ",
          "trailing_diamond": "\ue0c6",
          "type": "time"
        },
        {
          "background": "#2e9599",
          "background_templates": [
            "{{ if gt .Code 0 }}#f1184c{{ end }}"
          ],
          "foreground": "#ffffff",
          "leading_diamond": "\ue0c7",
          "properties": {
            "always_enabled": true
          },
          "style": "diamond",
          "template": " \ue23a{{ if gt .Code 0 }}\uf00d{{ else }}\uf42e{{ end }} ",
          "trailing_diamond": "\ue0c6",
          "type": "exit"
        },
        {
          "background": "#bdbdbd",
          "foreground": "#193549",
          "leading_diamond": "\ue0c7",
          "style": "diamond",
          "template": " \ue7a8 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "trailing_diamond": "\ue0c6",
          "type": "rust"
        },
        {
          "background": "#ff5e00",
          "foreground": "#000000",
          "leading_diamond": "\ue0c7",
          "properties": {
            "style": "austin",
            "threshold": 1000
          },
          "style": "diamond",
          "template": "\u231b{{ .FormattedMs }} ",
          "trailing_diamond": "\ue0c6",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
